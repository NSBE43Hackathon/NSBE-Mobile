<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_chapters__r2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.chels.nsbe_network.Chapters_R2"
    tools:showIn="@layout/app_bar_chapters__r2">

    <Button
        android:text="Lehigh University"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/chapter_lu"
        android:onClick="ButtonOnClick"
        />

    <Button
        android:text="Temple"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/chapter_lu"
        android:id="@+id/chapter_temple"
        />

    <Button
        android:text="University of Pittsburgh"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/chapter_temple"
        android:id="@+id/chapter_upitt"
        />

</RelativeLayout>
