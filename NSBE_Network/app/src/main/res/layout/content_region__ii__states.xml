<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_region__ii__states"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.chels.nsbe_network.Region_II_States"
    tools:showIn="@layout/app_bar_region__ii__states">

    <Button
        android:text="Delaware"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/connecticut"
        android:id="@+id/delaware" />

    <Button
        android:text="Maryland"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/delaware"
        android:id="@+id/maryland" />

    <Button
        android:text="North Carolina"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/maryland"
        android:id="@+id/north_carolina" />

    <Button
        android:text="Pennsylvania"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/north_carolina"
        android:id="@+id/pennsylvania"
        android:onClick="ButtonOnClick"
        />

    <Button
        android:text="South Carolina"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/pennsylvania"
        android:id="@+id/south_carolina" />

    <Button
        android:text="Virginia"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/south_carolina"
        android:id="@+id/virginia" />

    <Button
        android:text="West Virginia"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/virginia"
        android:id="@+id/west_virginia" />
</RelativeLayout>
